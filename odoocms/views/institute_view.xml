<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_odoocms_institute_form" model="ir.ui.view">
        <field name="name">odoocms.institute.form</field>
        <field name="model">odoocms.institute</field>
        <field name="inherit_id" ref="odoocms_base.view_odoocms_institute_form"/>
        <field name="arch" type="xml">
            <field name="effective_date" position="before">
                <field name="dean_id"
                    options="{'no_create': True, 'no_quick_create':True, 'no_create_edit': True, 'limit': 10}"/>
                <field name="company_id"/>
            </field>
            <page name="childs" position="after">
                <page name="faculty" string="Faculty Staff">
                    <field name="faculty_ids" colspan="4" context="{'default_institute_id':id}">
                        <tree editable="bottom">
                            <field name="employee_id"/>
                            <field name="employee_tag_id" widget="many2many_tags"/>
                            <field name="employee_name"/>
                            <field name="employee_work_phone"/>
                            <field name="employee_work_email"/>
                            <field name="employee_department_id"/>
                            <field name="employee_job_id"/>
                            <field name="employee_parent_id"/>
                        </tree>
                        <form>
                            <group>
                                <group>
                                    <field name="employee_id"/>
                                    <field name="employee_name"/>
                                </group>
                                <group>
                                    <field name="employee_work_phone"/>
                                    <field name="employee_work_email"/>
                                </group>
                                <group>
                                    <field name="employee_department_id"/>
                                    <field name="employee_job_id"/>
                                    <field name="employee_parent_id"/>
                                </group>
                            </group>
                        </form>
                    </field>
                </page>
                <page name='other_info' string="Other Info">
                    <group>
                        <group string="Buildings">
                            <field name="building_ids" widget="many2many_tags" nolabel="1"/>
                        </group>
                        <group name="fee_info"/>
                        <group name="fine_info"/>
                    </group>
                </page>
            </page>
        </field>
    </record>


</odoo>