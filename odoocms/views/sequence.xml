<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">





        <record id="seq_student_student" model="ir.sequence">
            <field name="name">Student Admission</field>
            <field name="code">odoocms.student</field>
            <field name="prefix">S</field>
            <field name="padding">3</field>
            
        </record>

        <record id="seq_odoocams_faculty_staff" model="ir.sequence">
            <field name="name">Faculty Staff</field>
            <field name="code">odoocms.faculty.staff</field>
            <field name="prefix">FS</field>
            <field name="padding">4</field>

        </record>

        <record id="seq_odoocms_documents" model="ir.sequence">
            <field name="name">Student Documents</field>
            <field name="code">odoocms.documents</field>
            <field name="prefix">DOC/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field name="padding">3</field>
        </record>

