<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="blacklist_applicant_view_tree" model="ir.ui.view">
        <field name="name">admission.black.list.view.tree</field>
        <field name="model">admission.blacklist.application</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name" />
                <field name="cnic" />
                <field name="dob" />
                <field name="description" string='Reason' />
                <field name="date" />
            </tree>
        </field>
    </record>
    <record id="blacklist_application_action" model="ir.actions.act_window">
        <field name="name">BlackList Menu</field>
        <field name="res_model">admission.blacklist.application</field>
        <field name="view_mode">tree</field>
    </record>
    <menuitem
        id="blacklist_applicant_menu"
        name="Blacklist Applicant"
        action="blacklist_application_action"
        parent="admission_applicant_menu"
        />
</odoo>