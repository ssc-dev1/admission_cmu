<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="base_view_company_form_view_form_inherit" model="ir.ui.view">
            <field name="name">base.view_company_form_view_form.inherit</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form" />
            <field name="arch" type="xml">
                 <field name="name" position="after">
                        <field name="company_code" />
                </field>

                <field name="favicon" position="after">
                        <field name="admission_banner" widget="Image"/>
                </field>
            </field>
        </record>


        <record id="odoocms_admission_view_odoocms_application_form" model="ir.ui.view">
            <field name="name">	odoocms_admission.view_odoocms_application_form</field>
            <field name="model">odoocms.application</field>
            <field name="inherit_id" ref="odoocms_admission.view_odoocms_application_form" />
            <field name="arch" type="xml">
                <field name="transfer_program_id" position="after">
                        <field name="company_id" />
                </field>
            </field>
        </record>

    </data>
</odoo>