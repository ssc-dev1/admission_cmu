<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="view_inherited_allocate_slot_wizard_form" model="ir.ui.view">
        <field name="name">inherited.allocate.slot.wizard</field>
        <field name="model">allocate.slot.wizard</field>
        <field name="inherit_id" ref="odoocms_admission.allocate_slot_wizard_form"/>
        <field name="arch" type="xml">

            <xpath expr="//notebook/page[@name='allocate_slot_page']/field[@name='applicant_ids']" position="replace">
                <field name="applicant_ids" domain="[('register_id', '=', register_id),('fee_voucher_state', '=', 'verify'),('state', 'not in', ('draft','done','reject'))]"/>
            </xpath>
        </field>
    </record>
</odoo>