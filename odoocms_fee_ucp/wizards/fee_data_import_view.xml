<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="inherited_view_fee_data_import_wizard_form" model="ir.ui.view">
            <field name="name">inherited.fee.data.import.form.view</field>
            <field name="model">fee.data.import.wizard</field>
            <field name="inherit_id" ref="odoocms_fee_ext.view_fee_data_import_wizard_form"/>
