<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_integration_odoocms_program_tree" model="ir.ui.view">
        <field name="name">integration.odoocms.program.tree</field>
        <field name="model">odoocms.program</field>
        <field name="inherit_id" ref="odoocms_base.view_odoocms_program_tree"/>
        <field name="arch" type="xml">
            <field name="active" position="after">
                <field name="integration_code"/>
            </field>
        </field>
    </record>

    <record id="view_integration_odoocms_program_form" model="ir.ui.view">
        <field name="name">integration.odoocms.program.form</field>
        <field name="model">odoocms.program</field>
        <field name="inherit_id" ref="odoocms_base.view_odoocms_program_form"/>
        <field name="arch" type="xml">
            <field name="campus_id" position="after">
                <field name="integration_code" required="1"/>
            </field>
        </field>
    </record>

    <record id="view_integration_odoocms_program_search" model="ir.ui.view">
        <field name="name">integration.odoocms.program.search</field>
        <field name="model">odoocms.program</field>
        <field name="inherit_id" ref="odoocms_base.view_odoocms_program_search"/>
        <field name="arch" type="xml">
            <field name="code" position="after">
                <field name="integration_code"/>
            </field>
        </field>
    </record>


</odoo>
