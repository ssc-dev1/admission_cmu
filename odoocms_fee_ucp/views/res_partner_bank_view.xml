<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_integration_partner_bank_form" model="ir.ui.view">
        <field name="name">.integration.res.partner.bank.form</field>
        <field name="model">res.partner.bank</field>
        <field name="inherit_id" ref="base.view_partner_bank_form"/>
        <field name="arch" type="xml">
            <field name="active" position="after">
                <field name="bank_account_code" required="1"/>
                <field name="bank_ledger_code" required="1"/>
            </field>
        </field>
    </record>

</odoo>
