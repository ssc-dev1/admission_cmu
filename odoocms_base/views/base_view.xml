                <field name="latitude"/>
                <field name="longitude"/>
            </tree>
        </field>
    </record>
    <record id="view_odoocms_city_form" model="ir.ui.view">
        <field name="name">odoocms.city.form</field>
        <field name="model">odoocms.city</field>
        <field name="arch" type="xml">
            <form string="City">
                <sheet>
                    <group string="City">
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                        </group>
                        <group>
                            <field name="district_id"/>
                            <field name="province_id"/>
                        </group>
                        <group>
                            <field name="postal_code"/>
                        </group>
                        <group>
                            <field name="latitude"/>
                            <field name="longitude"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="view_odoocms_city_search" model="ir.ui.view">
        <field name="name">odoocms.city.search</field>
        <field name="model">odoocms.city</field>
        <field name="priority" eval="14"/>
        <field name="arch" type="xml">
            <search string="City">
                <field name="name"/>
                <field name="code"/>
                <field name="district_id"/>
                <field name="province_id"/>
                <field name="postal_code"/>
                <newline/>
                <group expand="0" string="Group By...">
                    <filter string="Province" domain="[]" name="province_grp" context="{'group_by':'province_id'}" />
                    <filter string="District" domain="[]" name="district_grp" context="{'group_by':'district_id'}" />
                </group>
            </search>
        </field>
    </record>
    <record id="action_odoocms_city" model="ir.actions.act_window">
