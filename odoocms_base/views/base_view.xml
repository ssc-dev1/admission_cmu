    <record id="view_odoocms_religion_tree" model="ir.ui.view">
        <field name="name">odoocms.religion.tree</field>
        <field name="model">odoocms.religion</field>
        <field name="arch" type="xml">
            <tree string="Odoo CMS Religion" editable="bottom">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
                <field name="code"/>
            </tree>
        </field>
    </record>
    <record id="view_odoocms_religion_form" model="ir.ui.view">
        <field name="name">odoocms.religion.form</field>
        <field name="model">odoocms.religion</field>
        <field name="arch" type="xml">
            <form string="Odoo CMS Religion">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                        </group>
                        <group>
                            <field name="code"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="action_odoocms_religion_view" model="ir.actions.act_window">
        <field name="name">Religion</field>
        <field name="res_model">odoocms.religion</field>
