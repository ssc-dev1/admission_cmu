    <record id="view_odoocms_province_tree" model="ir.ui.view">
        <field name="name">odoocms.province.tree</field>
        <field name="model">odoocms.province</field>
        <field name="arch" type="xml">
            <tree string="Province">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
                <field name="code"/>
                <field name="country_id"/>
            </tree>
        </field>
    </record>
    <record id="view_odoocms_province_form" model="ir.ui.view">
        <field name="name">odoocms.province.form</field>
        <field name="model">odoocms.province</field>
        <field name="arch" type="xml">
            <form string="Province">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                        </group>
                        <group>
                            <field name="country_id"/>
                        </group>
                    </group>
                    <notebook>
                        <page string="Districts" name="districts">
                            <field name="district_ids">
                                <tree editable="bottom">
                                    <field name="sequence" widget="handle"/>
                                    <field name="name"/>
                                    <field name="code"/>
                                </tree>
                            </field>
                        </page>
                        <page string="Domicile">
                            <field name="domicile_ids">
                                <tree string="Domicile" editable="bottom">
                                    <field name="sequence" widget="handle"/>
                                    <field name="name"/>
                                    <field name="code"/>
                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
