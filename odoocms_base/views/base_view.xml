    <record id="view_odoocms_city_tree" model="ir.ui.view">
        <field name="name">odoocms.city.tree</field>
        <field name="model">odoocms.city</field>
        <field name="arch" type="xml">
            <tree string="City" editable="bottom">
                <field name="sequence" widget="handle"/>
                <field name="code"/>
                <field name="name"/>
