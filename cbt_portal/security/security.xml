<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="module_category_cbt_portal" model="ir.module.category">
            <field name="name">CBT Portal</field>
            <field name="description">Helps you handle your CBT Portal needs.</field>
            <field name="sequence">10</field>
        </record>

        <record id="group_cbt_manager" model="res.groups">
            <field name="name">CBT Manager</field>
            <field name="category_id" ref="module_category_cbt_portal"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
        <record id="group_cbt_user" model="res.groups">
            <field name="name">CBT User</field>
            <field name="category_id" ref="module_category_cbt_portal"/>
            <field name="implied_ids" eval="[(4, ref('cbt_portal.group_cbt_manager'))]"/>
        </record>



