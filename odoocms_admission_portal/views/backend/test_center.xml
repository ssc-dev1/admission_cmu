<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_test_center_tree" model="ir.ui.view">
            <field name="name">test.center.tree</field>
            <field name="model">test.center</field>
            <field name="arch" type="xml">
                <tree string="Entry Test">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="city"/>
                    <field name="note"/>
                </tree>
            </field>
        </record>
        <record id="view_test_center__form" model="ir.ui.view">
            <field name="name">test.center.form</field>
            <field name="model">test.center</field>
            <field name="arch" type="xml">
                <form string="Test Center">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="code"/>
                            </group>
                            <group>
                                <field name="city"/>
                                <field name="sequence"/>
                                <field name="note"/>
                            </group>
                        </group>
                       
                    </sheet>
                </form>
            </field>
        </record>
        <record id="action_test_center" model="ir.actions.act_window">
            <field name="name">Test Center</field>
            <field name="res_model">test.center</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Create Your Test Center here.
                </p>
            </field>
        </record>

        <menuitem
                id="menu_action_test_center"
                name="Test Center"
                parent="odoocms_admission.menu_admission_configuration"
                action="action_test_center"
                groups="odoocms_admission.group_admission_manager"
                sequence="180"/>
      
    </data>

</odoo>