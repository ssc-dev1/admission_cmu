<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="seq_application_steps" model="ir.sequence">
        <field name="name">Application Main Steps</field>
        <field name="code">application.main.steps</field>
        <field name="prefix"></field>
        <field name="padding">3</field>
    </record>
        <record id="portal_url_encryption" model="ir.config_parameter">
            <field name="key">portal_url_encryption</field>
            <field name="value">True</field>
    </record>
    <record id="test_center_portal_option" model="ir.config_parameter">
            <field name="key">test_center_portal_option</field>
            <field name="value">False</field>
    </record>
    <record id="record_rule_test_center_portal" model="ir.rule">
        <field name="name">Test Center multi company record rule</field>
        <field name="model_id" ref="model_test_center"/>
        <field name="domain_force">['|',('company_id','=',False),('company_id', 'in', company_ids)]</field>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="True"/>
        <field name="perm_create" eval="True"/>
        <field name="perm_unlink" eval="False"/>
    </record>
</odoo>