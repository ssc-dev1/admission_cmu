<?xml version='1.0' encoding='utf-8'?>
<odoo>



    <record id="class_activity_report_wizard_form" model="ir.ui.view">
        <field name="name">class.activity.report.form</field>
        <field name="model">class.activity.report</field>
        <field name="arch" type="xml">
            <form string="Class Activity Report">
                    <group>
                        <group>
                            <field name="faculty_id"/>
                            <field name="term_id"/>
                        </group>
                    </group>
                <footer>
                    <button name="print_excel_report" string="Print Excel Report" type="object" class="btn-primary"/>
                    <button name="print_report" string="Print Report" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>

    <record id="class_activity_report_action" model="ir.actions.act_window">
        <field name="name">Class Activity Report</field>
        <field name="res_model">class.activity.report</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <menuitem
            id="course_load_menu_cms"
            name="Class Activity Report"
            action="class_activity_report_action"
            sequence="200"
            parent="menu_odoocms_class_activities_root" />

   
    
</odoo>
