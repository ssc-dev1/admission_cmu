<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_company_form_aarsol_base" model="ir.ui.view">
        <field name="name">res.company.form.aarsol.base</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <field name="favicon" position="after">
                <field name="identifier"/>
                <field name="short_name"/>
                <field name="company_tag"/>
                <field name="logo_width"/>
                <field name="logo_height"/>
            </field>
        </field>
    </record>

</odoo>