<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="view_odoocms_process_fee_payment_form" model="ir.ui.view">
		<field name="name">odoocms.process.fee.payment.form</field>
		<field name="model">odoocms.process.fee.payment</field>
		<field name="arch" type="xml">
			<form string="Process Fee Payment">
				<group>
					<field name="tag"/>
				</group>
				<group>
					<field name="payment_ids" colspan="4" nolabel="1"/>
				</group>
				<footer>
					<button name="process_payment" string="Process Payment" type="object" class="btn-primary"/>
					<button string="Cancel" class="btn-default" special="cancel"/>
				</footer>
		   </form>
		</field>
	</record>

	<record id="odoocms_process_fee_payment_action" model="ir.actions.act_window">
		<field name="name">Process Fee Payment</field>
		<field name="res_model">odoocms.process.fee.payment</field>
		<field name="view_mode">form</field>
		<field name="target">new</field>
		<field name="binding_model_id" ref="odoocms_fee.model_odoocms_fee_payment"/>
	</record>
		
</odoo>
