<?xml version="1.0"?>
<odoo>

    <record id="dynamic_head_wise_fee_received_summary_rep_wiz_view" model="ir.ui.view">
        <field name="name">dynamic.head.wise.fee.received.summary.rep.wiz.view</field>
        <field name="model">dynamic.head.wise.fee.received.summary.rep.wiz</field>
        <field name="arch" type="xml">
            <form string="Dynamic Head Wise Fee Received Summary Report">
                <group>
                    <group>
                        <field name="date_from" required="1"/>
                        <field name="date_to" required="1"/>
                        <field name="include_surplus"/>
                    </group>
                    <group>

                    </group>
                </group>
                <footer>
                    <button name="make_excel" string="Generate Excel Report" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_dynamic_head_wise_fee_received_summary_rep_wiz" model="ir.actions.act_window">
        <field name="name">Dynamic Head Wise Fee Received Summary Report</field>
        <field name="res_model">dynamic.head.wise.fee.received.summary.rep.wiz</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>


    <menuitem
            id="menu_dynamic_head_wise_fee_received_summary_rep_wiz"
            name="Dynamic Head Wise Fee Received Summary"
            action="action_dynamic_head_wise_fee_received_summary_rep_wiz"
            parent="odoocms_fee.menu_dynamic_fee_reports"
            groups="odoocms_fee.group_fee_user,odoocms_fee.group_fee_coordinator,odoocms_fee.group_fee_manager"
            sequence="10"/>

</odoo>
