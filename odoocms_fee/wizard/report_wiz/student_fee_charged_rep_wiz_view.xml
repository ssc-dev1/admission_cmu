<?xml version="1.0"?>
<odoo>

    <record id="student_fee_charged_rep_wiz_view" model="ir.ui.view">
        <field name="name">student.fee.charged.rep.wiz.view</field>
        <field name="model">student.fee.charged.rep.wiz</field>
        <field name="arch" type="xml">
            <form string="Student Fee Charged Report">
                <group>
                    <group>
                        <field name="term_id" required="1" options="{'no_quick_create':True,'no_create_edit':True,'no_create':True}"/>
                        <field name="institute_ids" required="1" widget="many2many_tags" options="{'no_quick_create':True,'no_create_edit':True,'no_create':True}"/>
                    </group>
                    <group>
                    </group>
                </group>
                <footer>
                    <button name="make_excel" string="Generate Report" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_student_fee_charged_rep_wiz" model="ir.actions.act_window">
        <field name="name">Student Fee Charged Report</field>
        <field name="res_model">student.fee.charged.rep.wiz</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>


    <menuitem
            id="menu_student_fee_charged_rep_wiz"
            name="Student Fee Charged Report"
            action="action_student_fee_charged_rep_wiz"
            parent="odoocms_fee.menu_fee_reports"
            groups="odoocms_fee.group_fee_user,odoocms_fee.group_fee_coordinator,odoocms_fee.group_fee_manager"
            sequence="400"/>

</odoo>
