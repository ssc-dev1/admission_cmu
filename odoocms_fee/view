        <record id="odoocms_challan_filter" model="ir.ui.view">
            <field name="name">odoocms.fee.barcode.select</field>
            <field name="model">odoocms.fee.barcode</field>
            <field name="arch" type="xml">
                <search string="Search Challans">
                    <field name="student_id"/>
                    <field name="name" string="Challan Ref"/>
                    <field name="tag_ids"/>
                    <separator/>
                    <field name="program_id" string="Program"/>
                    <field name="term_id" string="Term"/>
                    <separator/>
                    <field name="date" string="Invoice Date"/>
                    <field name="date_due" string="Invoice Due Date"/>
                    <field name="date_payment" string="Payment Date"/>
                    <field name="date_post" string="Post Date"/>
                    <separator/>

                    <filter name="unpaid" string="Unpaid" domain="[('state','=','draft')]"/>
                    <filter name="paid" string="Paid" domain="[('state', '=', 'paid')]"/>
                    <filter name="late" string="Overdue"
                            domain="['&amp;', ('date_due', '&lt;', time.strftime('%%Y-%%m-%%d')), ('state', '=', 'draft')]"
                            help="Overdue invoices, maturity date passed"/>
                    <filter name="filter_payment_date" string="Payment Date" date="date_payment"/>

                    <separator/>
                    <group expand="0" string="Group By">
                        <filter name="group_by_academic_term" string="Academic Term" context="{'group_by':'term_id'}"/>
                        <separator/>
                        <filter name="group_by_campus" string="Campus" context="{'group_by':'campus_id'}"/>
                        <filter name="group_by_institute" string="Institute" context="{'group_by':'institute_id'}"/>
                        <filter name="group_by_session" string="Session" context="{'group_by':'session_id'}"/>
                        <filter name="group_by_career" string="Career" context="{'group_by':'career_id'}"/>
                        <filter name="group_by_discipline" string="Discipline" context="{'group_by':'discipline_id'}"/>
                        <filter name="group_by_student" string="Student" context="{'group_by':'student_id'}"/>
                        <filter name="group_by_invoice_status" string="Status" context="{'group_by':'state'}"/>
                        <separator/>
                        <filter name="group_by_invoice_date" string="Receipt Date" context="{'group_by':'date'}"/>
                        <filter name="group_by_due_date" string="Due Date" context="{'group_by':'date_due'}"/>
                        <filter name="group_by_payment_date" string="Payment Date" context="{'group_by':'date_payment'}"/>
                        <filter name="group_by_post_date" string="Post Date" context="{'group_by':'date_post'}"/>
                        <separator/>
                        <filter name="group_by_tag_ids" string="Tags" context="{'group_by':'tag_ids'}"/>
                        <filter name="group_by_waiver_ids" string="Waiver Type" context="{'group_by':'waiver_ids'}"/>
                        <filter name="group_by_waiver_percentage" string="Waiver Percentage" context="{'group_by':'waiver_percentage'}"/>
                        <filter name="group_by_label_id" string="Challan Type" context="{'group_by':'label_id'}"/>
                        <filter name="group_by_journal_id" string="Bank/Journal" context="{'group_by':'journal_id'}"/>
                        <filter name="group_by_fine_policy_line_id" string="Fine Policy" context="{'group_by':'fine_policy_line_id'}"/>
