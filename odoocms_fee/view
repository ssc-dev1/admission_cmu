        <record id="odoocms_challan_dashboard" model="ir.ui.view">
            <field name="name">odoocms.fee.barcode.dashboard</field>
            <field name="model">odoocms.fee.barcode</field>
            <field name="mode">primary</field>
            <field name="arch" type="xml">
                <dashboard sample="1">
                    <view type="graph" ref="odoocms_fee.odoocms_challan_graph"/>
                    <group>
                        <group>
                            <aggregate name="challans" string="Challans" group_operator="count_distinct" field="id" measure="__count__"/>
                            <aggregate name="expected_revenue" string="Expected Revenue" field="amount" group_operator="sum" widget="monetary"/>
                            <aggregate name="received_revenue" string="Received Revenue" field="amount" domain="[('state','=','paid')]" group_operator="sum" widget="monetary"/>
                            <aggregate name="paid" string="Paid" field="id" domain="[('state','=','paid')]" group_operator="count" invisible="1"/>
<!--                            <aggregate name="days_to_convert" string="Days To Opportunity" group_operator="avg" field="days_to_convert"/>-->
<!--                            <aggregate name="days_to_assign" string="Days to Assign" field="day_open" group_operator="avg" value_label="days"/>-->
                            <formula name="paid_percent" string="% Paid" value="record.paid / record.challans" widget="percentage"/>

<!--                            <aggregate name="prorated_revenue" string="Prorated Revenue" field="prorated_revenue" group_operator="sum" widget="monetary"/>-->
<!--                            <aggregate name="day_close" string="Days to Close" field="day_close" group_operator="avg" value_label="days"/>-->

                            <aggregate name="main" string="Main Challan" field="amount" domain="[('label_id.type','=','main')]" group_operator="sum"/>
                            <aggregate name="second" string="2nd Challan" field="amount" domain="[('label_id.type','=','installment')]" group_operator="sum"/>
                        </group>
                        <group>
                            <widget name="pie_chart" title="Waiver Percentage" attrs="{'groupby': 'waiver_percentage'}"/>
                            <widget name="pie_chart" title="Payment Status" attrs="{'groupby': 'state'}"/>
                            <widget name="pie_chart" title="Installment" attrs="{'groupby': 'label_id'}"/>
                        </group>
                    </group>
                    <view type="pivot" ref="odoocms_fee.odoocms_challan_pivot"/>
                    <view type="cohort" ref="odoocms_fee.odoocms_challan_cohort"/>
                </dashboard>
            </field>
        </record>

