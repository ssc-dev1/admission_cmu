<?xml version="1.0"?>
<odoo>
    <data noupdate="1">

        <record id="sequence_odoocms_show_challan_on_portal" model="ir.sequence">
            <field name="name">Show Challan On Portal Seq</field>
            <field name="code">odoocms.show.challan.on.portal</field>
            <field name="padding">4</field>
        </record>

        <record id="sequence_odoocms_show_challan_on_portal_line" model="ir.sequence">
            <field name="name">Show Challan On Portal Line Seq</field>
            <field name="code">odoocms.show.challan.on.portal.line</field>
            <field name="padding">4</field>
        </record>


        <!--***** Email template *****-->
        <record id="registered_student_email_template" model="mail.template">
            <field name="name">Registered Student Email Template</field>
            <field name="email_from">{{ctx.get('admission_mail')}}</field>
            <field name="subject">Registered Students</field>
            <field name="email_to">{{ctx.get('mail_to')}}</field>
            <field name="model_id" ref="odoocms_admission_fee_ucp.model_odoocms_fee_payment"/>
        </record>

    </data>
</odoo>
