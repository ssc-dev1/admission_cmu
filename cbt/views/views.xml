                </tree>
            </field>
        </record>
        <record id="cbt_program_courses_form" model="ir.ui.view">
            <field name="name">cbt.program.courses.form</field>
            <field name="model">cbt.program.courses</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="etest_id"/>
                                <field name="type_id"/>
                            </group>
                            <group>
                                <field name="program_id"/>
                            </group>
                        </group>
                        <notebook>
                            <page name="Subjects" string="Subjects">
                                <field name="subject_ids" widget="many2many" options="{'no_create': True}"/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <!--MCQs Setup-->
        <record model="ir.ui.view" id="cbt_mcqs_tree">
            <field name="name">Add MCQs</field>
            <field name="model">cbt.mcqs</field>
            <field name="arch" type="xml">
                <tree string="Question Bank">
                    <field name="question_title"/>
                    <field name="subject_id"/>
                    <field name="topic_id"/>
                    <field name="stopic_id"/>
                    <field name="difficulty_level_id"/>
                    <field name="type_id"/>
                    <field name="state"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="cbt_mcqs_form">
            <field name="name">Add MCQs</field>
            <field name="model">cbt.mcqs</field>
            <field name="arch" type="xml">
                <form>
                    <header>
                        <field name="state" widget="statusbar"
                               statusbar_visible="draft,approved,cancel"/>
                        <field name="subject_code" hidden="true"/>
                        <field name="type_code" hidden="true"/>
                    </header>

                    <sheet>
                        <div class="oe_button_box" name="button_box">
                        <button type="object"
                                name="action_view_child_questions"
                                class="oe_stat_button"
                                icon="fa-building"> <!--attrs="{'invisible':[('picking_ids','=',[])]}-->
                            <field name="count" widget="statinfo" string="Scenario Questions" help="Scenario Questions List"/>
                            <!--<field name="picking_ids" invisible="1"/>-->
                        </button>

                    </div>
                        <group>
                            <group>
                                <field name="subject_id" />
                                <field name="scenario_relation" attrs="{ 'invisible': [('subject_code','!=','scenario')], 'required': [('subject_code','=','scenario')]}" />
                                <field name="scenario_id" attrs="{'invisible': [('scenario_relation','in',['parent',False])] }"/>
                                <field name="topic_id" attrs="{ 'invisible': [('subject_code','=','scenario')] }"/>
                                <field name="stopic_id" attrs="{ 'invisible': [('subject_code','=','scenario')] }"/>
                                <field name="current_user"/>
                                <field name="reviewed_user" readonly="True"/>
                            </group>
                            <group>
                                <field name="type_id" attrs="{ 'invisible': [('scenario_relation','=','parent')], 'required': [('scenario_relation','!=','parent')]}"/>
                                <field name="skill_id" attrs="{ 'invisible': [('scenario_relation','=','child')]}"/>
                                <field name="difficulty_level_id" attrs="{ 'invisible': [('scenario_relation','=','child')], 'required': [('scenario_relation','!=','child')]}" />
                                <field name="time_id" attrs="{ 'invisible': [('scenario_relation','=','child')]}"/>
                            </group>
                        </group>
                        <group>
                            <field name="question_title" required="True"/>
                            <field name="question" widget="html"/>
                        </group>
                        <group>
                            <group>
                                <!--<field name="scenario_relation" attrs="{ 'invisible': [('type_id','!=',3)] }"/>-->


                                <field name="option_quantity" attrs="{ 'invisible': ['|',('scenario_relation','=','parent'),('type_code','=','3')] }"/>
                            </group>
                        </group>
                        <notebook>
                            <page name="Options" string="Options" attrs="{ 'invisible': ['|',('type_code','=','3'),('scenario_relation','=','parent')] }">
                                <group>
                                    <field name="ans1_correct"/>
                                    <field name="ans1"/>
                                </group>
                                <group>
                                    <field name="ans2_correct"/>
                                    <field name="ans2"/>
                                </group>
                                <group attrs="{ 'invisible': [('option_quantity','=','2')] }">
                                    <field name="ans3_correct"/>
                                    <field name="ans3_correct"/>
                                    <field name="ans3" />
                                </group>
                                <group attrs="{ 'invisible': [('option_quantity','in',('2','3'))] }">
                                    <field name="ans4_correct"/>
                                    <field name="ans4"/>
                                </group>
                                <group attrs="{ 'invisible': [('option_quantity','in',('2','3','4'))] }">
                                    <field name="ans5_correct" />
                                    <field name="ans5" />
                                </group>
                                <group attrs="{ 'invisible': [('option_quantity','in',('2','3','4','5'))] }">
                                    <field name="ans6_correct"/>
                                    <field name="ans6"/>
                                </group>
                            </page>
                            <page name="Question Track" string="Question Track">
                                <field name="question_repeat_ids">
                                    <tree delete="false" create="false">
                                        <field name="date" string="Date"/>
                                        <field name="paper_id" string="Paper"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="cbt_mcqs_search" model="ir.ui.view">
            <field name="name">cbt.mcqs.search</field>
