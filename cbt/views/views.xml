        <!--Sessions & Training-->
        <record id="cbt_session_tree" model="ir.ui.view">
            <field name="name">cbt.session.tree</field>
            <field name="model">cbt.session</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="sequence" widget="handle"/>
                    <field name="etest_id"/>
                    <field name="type_id"/>
                </tree>
            </field>
        </record>
        <record id="cbt_session_form" model="ir.ui.view">
            <field name="name">cbt.session.form</field>
            <field name="model">cbt.session</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="etest_id"/>
                            </group>
                            <group>
                                <field name="type_id"/>
                            </group>
                        </group>
                        <notebook>
                            <page name="Timings" string="Timings">
                                <field name="line_ids">
                                    <tree editable="bottom">
                                        <field name="date"/>
                                        <field name="session_no"/>
                                        <field name="time" widget="float_time"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers" groups="base.group_user"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
