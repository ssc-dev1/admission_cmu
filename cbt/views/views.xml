                        <filter name="stopic_id" string="Sub Topics" domain="[]" context="{'group_by':'stopic_id'}"/>
                        <filter name="difficulty_level_id" string="Questions Difficulty Level" domain="[]"
                                context="{'group_by':'difficulty_level_id'}"/>
                        <filter name="state" string="Questions Status" domain="[]" context="{'group_by':'state'}"/>
                    </group>
                </search>
            </field>
        </record>

