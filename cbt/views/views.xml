<odoo>
    <data>

        <!--programs-->
        <record id="cbt_program_tree" model="ir.ui.view">
            <field name="name">cbt.program.tree</field>
            <field name="model">cbt.program</field>
            <field name="arch" type="xml">
                <tree string="program" editable="bottom">
                    <field name="sequence" widget="handle"/>
                    <field name="code"/>
                    <field name="name"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="cbt_program_search" model="ir.ui.view">
            <field name="name">cbt.program.search</field>
            <field name="model">cbt.program</field>
            <field name="priority" eval="14"/>
            <field name="arch" type="xml">
                <search string="program">
                    <field name="name"/>
                    <field name="code"/>
                    <newline/>

                </search>
            </field>
        </record>


        <!--Subjects-->
        <record id="cbt_sub_tree" model="ir.ui.view">
            <field name="name">cbt.subject.tree</field>
            <field name="model">cbt.subject</field>
            <field name="arch" type="xml">
                <tree create="false" delete="false" edit="false" string="Subject/Section">
                    <field name="sequence" widget="handle"/>
                    <field name="code"/>
                    <field name="name"/>
                    <field name="topic_count"/>
                    <field name="sub_topic_count"/>
                </tree>
            </field>
        </record>
        <record id="cbt_sub_form" model="ir.ui.view">
            <field name="name">cbt.subject.form</field>
            <field name="model">cbt.subject</field>
            <field name="arch" type="xml">
                <form create="false" delete="false" edit="false" string="Subject/Section">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" required="1"/>
                            </group>
                            <group>
                                <field name="code" required="1"/>
                            </group>
                        </group>
                        <notebook>
                            <page name="topic" string="Topic">
                                <field name="topic_ids">
                                    <field name="sequence" widget="handle"/>
                                    <field name="code"/>
                                    <field name="name"/>
                                </field>
                            </page>
                            <page name="role" string="Faculty Role">
                                <field name="role_ids"/>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="cbt_sub_search" model="ir.ui.view">
            <field name="name">cbt.subject.search</field>
            <field name="model">cbt.subject</field>
            <field name="priority" eval="14"/>
            <field name="arch" type="xml">
                <search string="Employee Roles">
                    <field name="name"/>
                    <field name="code"/>
                    <newline/>
                </search>
            </field>
        </record>

        <!--Topics-->
        <record id="cbt_subject_topic_tree" model="ir.ui.view">
            <field name="name">cbt.subject.topic.tree</field>
            <field name="model">cbt.subject.topic</field>
            <field name="arch" type="xml">
                <tree string="Topic" >
                    <field name="sequence" widget="handle"/>
                    <field name="code"/>
                    <field name="name"/>
                    <field name="subject_id"/>
                    <field name="sub_topic_count"/>
                </tree>
            </field>
        </record>
        <record id="cbt_topic_form" model="ir.ui.view">
            <field name="name">cbt.subject.topic.form</field>
            <field name="model">cbt.subject.topic</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="name" required="1"/>
                                <field name="code" required="1"/>
                            </group>
                            <group>
                                <field name="subject_id"
                                       options="{'no_quick_create':True,'no_create_edit':True,'no_open': True,'no_create':True,}"/>
                            </group>
                        </group>
                        <notebook>
                            <page name="sub_topic" string="Sub Topic">
                                <field name="sub_topic_ids">
                                    <tree editable="bottom">
                                        <field name="sequence" widget="handle"/>
                                        <field name="code" required="1"/>
                                        <field name="name" required="1"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="cbt_subject_topic_search" model="ir.ui.view">
            <field name="name">cbt.program.search</field>
            <field name="model">cbt.subject.topic</field>
            <field name="priority" eval="14"/>
            <field name="arch" type="xml">
                <search string="Topics">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="subject_id"/>
                    <newline/>
                    <group expand="0" string="Group By...">
                        <filter name="subject_id" string="Subjects" domain="[]" context="{'group_by':'subject_id'}"/>


                    </group>
                </search>
            </field>
        </record>

        <!--Sub Topic-->
        <record id="cbt_subtopic_tree" model="ir.ui.view">
            <field name="name">cbt.subject.subtopic.tree</field>
            <field name="model">cbt.subject.subtopic</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="sequence" widget="handle"/>
                    <field name="code" required="1"/>
                    <field name="name" required="1"/>
                    <field name="topic_id" required="1"/>
                    <field name="subject_id" required="1"/>
                </tree>
            </field>
        </record>



        <record id="cbt_subtopic_search" model="ir.ui.view">
            <field name="name">cbt.subject.subtopic.search</field>
            <field name="model">cbt.subject.subtopic</field>
            <field name="priority" eval="14"/>
            <field name="arch" type="xml">
                <search string="Sub Topics">
                    <field name="name"/>
                    <field name="code"/>
                    <field name="subject_id"/>
                    <field name="topic_id"/>
                    <newline/>
                    <group expand="0" string="Group By...">
                        <filter name="subject_id" string="Subjects" domain="[]" context="{'group_by':'subject_id'}"/>
                        <filter name="topic_id" string="Topics" domain="[]" context="{'group_by':'topic_id'}"/>


                    </group>
                </search>
            </field>
        </record>

        <!--Task Manager-->
        <record model="ir.ui.view" id="cbt_emloyee_jobs_tree">
            <field name="name">cbt.emloyee.jobs</field>
            <field name="model">cbt.jobs</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="date"/>
                    <field name="from_time" widget="float_time"/>
                    <field name="to_time" widget="float_time"/>
                    <field name="state"/>
                    <field name="active"/>
                </tree>
            </field>
        </record>
        <record model="ir.ui.view" id="cbt_emloyee_jobs_form">
            <field name="name">cbt.emloyee.jobs.setup</field>
            <field name="model">cbt.jobs</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="description"/>
                                <field name="date"/>
                                <field name="from_time" widget="float_time"/>
                                <field name="to_time" widget="float_time"/>
                            </group>
                            <group>
                                <field name="failure"/>
                                <field name="state"/>
                                <field name="active"/>
                                <field name="clear"/>
                            </group>
                        </group>
                    </sheet>

                </form>
            </field>
        </record>

        <!--Question Bank-->
        <!--Question Type-->
        <record id="cbt_question_type_tree" model="ir.ui.view">
            <field name="name">cbt.question.type.tree</field>
            <field name="model">cbt.question.type</field>
